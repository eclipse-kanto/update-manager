{
  "log": {
    "logFile": "log/update-manager.log",
    "logLevel": "ERROR",
    "logFileSize": 3,
    "logFileCount": 6,
    "logFileMaxAge": 29
  },
  "connection": {
    "broker":"www",
    "keepAlive": "500ms",
    "disconnectTimeout": "500ms",
    "username":"username",
    "password":"pass",
    "connectTimeout": "500ms",
    "acknowledgeTimeout": "500ms",
    "subscribeTimeout": "500ms",
    "unsubscribeTimeout": "500ms"
  },
  "domain": "mydomain",
  "thingsEnabled": false,
  "rebootEnabled": false,
  "rebootAfter": "1m",
  "reportFeedbackInterval": "2m",
  "currentStateDelay": "1m",
  "phaseTimeout": "2m",
  "ownerConsentCommands": ["DOWNLOAD"],
  "agents": {
    "self-update": {
      "rebootRequired": false,
      "readTimeout": "20s"
    },
    "containers": {
      "rebootRequired": true,
      "readTimeout": "30s"
    },
    "test-domain": {
      "rebootRequired": true,
      "readTimeout": "50s"
    }
  }
}